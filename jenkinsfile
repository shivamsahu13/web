pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                echo "========Building A========"
            }
            
        }
         stage("Test"){
            steps{
                echo "========Testing A========"
            }
            
        }
         stage("Staging"){
            steps{
                echo "========Staging A========"
            }
            
        }
         stage("Production"){
            steps{
                echo "========Production A========"
            }
            
        }
    }
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}